<?xml version="1.0"?>
<layout version="0.1.0">

<!--
Default layout, loads most of the pages
-->

    <default>
        
    </default>

    <customer_account_newsfeed>
        <update handle="customer_account"/>
        <reference name="root">
             <action method="setTemplate"><template>page/3columns.phtml</template></action>
            <action method="setHeaderTitle" translate="title" module="customer"><title>Newsfeed</title></action>
        </reference>
        <reference name="content">
            <block type="funstica/newsfeed" template="funstica/newsfeed.phtml" name="newsfeed" />
        </reference>
    </customer_account_newsfeed>
</layout>